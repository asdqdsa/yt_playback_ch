{
	"name": "Youtube play faster",
	"description": "Makes boring videos go faster than it should",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
  "permissions": ["storage", "activeTab", "scripting", "webNavigation"],
	"action": {
		"default_popup": "popup.html",
		"default_title": "Don't overstay my welcome human",
		"defaylt_icon": {
			"16": "/images/get_faster16.png",
      "32": "/images/get_faster32.png",
      "48": "/images/get_faster48.png",
      "128": "/images/get_faster128.png"
		},
		"commands": {
			"execute_action": {
				"suggested_key": {
					"default": "Shift+Q"
				},
				"description": "Does smth..."
			}
		}
	},
	"icons": {
		"16": "./images/get_faster16.png",
		"32": "./images/get_faster32.png",
		"48": "./images/get_faster48.png",
		"128": "./images/get_faster128.png"
	}
}
