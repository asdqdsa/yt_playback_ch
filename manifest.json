{
	"name": "Youtube play faster",
	"description": "Makes boring videos go faster than it should",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
  "permissions": ["storage", "activeTab", "scripting"],
	"action": {
		"default_popup": "popup.html",
		"defaylt_icon": {
			"16": "/images/get_faster16.png",
      "32": "/images/get_faster32.png",
      "48": "/images/get_faster48.png",
      "128": "/images/get_faster128.png"
		}
	},
	"icons": {
		"16": "./images/get_faster16.png",
		"32": "./images/get_faster32.png",
		"48": "./images/get_faster48.png",
		"128": "./images/get_faster128.png"
	}
}
